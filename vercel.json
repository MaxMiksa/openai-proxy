{
  "version": 2,
  "builds": [
    {
      "src": "api/openai/[...path].js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/openai/(.*)",
      "dest": "/api/openai/[...path].js"
    }
  ],
  "headers": [
    {
      "source": "/api/openai/(.*)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/markdown"
        }
      ]
    }
  ]
}
